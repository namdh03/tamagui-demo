{
  "expo": {
    "name": "tamagui-demo",
    "slug": "tamagui-demo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "tamaguidemo",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bitcode": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.namdh03.tamaguidemo"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "enableMinifyInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true
          }
        }
      ],
      [
        "expo-font",
        {
          "android": {
            "fonts": [
              {
                "fontFamily": "SFProDisplay-Regular",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProDisplay-Regular.ttf",
                    "weight": 400
                  }
                ]
              },
              {
                "fontFamily": "SFProDisplay-Medium",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProDisplay-Medium.ttf",
                    "weight": 500
                  }
                ]
              },
              {
                "fontFamily": "SFProDisplay-Semibold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProDisplay-Semibold.ttf",
                    "weight": 600
                  }
                ]
              },
              {
                "fontFamily": "SFProDisplay-Bold",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProDisplay-Bold.ttf",
                    "weight": 700
                  }
                ]
              },
              {
                "fontFamily": "SFProDisplay-Black",
                "fontDefinitions": [
                  {
                    "path": "./assets/fonts/SFProDisplay-Black.ttf",
                    "weight": 900
                  }
                ]
              }
            ]
          },
          "ios": {
            "fonts": [
              "./assets/fonts/SFProDisplay-Regular.ttf",
              "./assets/fonts/SFProDisplay-Medium.ttf",
              "./assets/fonts/SFProDisplay-Semibold.ttf",
              "./assets/fonts/SFProDisplay-Bold.ttf",
              "./assets/fonts/SFProDisplay-Black.ttf"
            ]
          }
        }
      ],
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
